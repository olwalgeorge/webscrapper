function initFreestart () {
  window.freestar = window.freestar || { queue: [], config: { enabled_slots: [] } };
  window.freestar.queue = window.freestar.queue || [];
  window.freestar.config = window.freestar.config || { enabled_slots: [] };
  window.freestar.config.enabled_slots = window.freestar.config.enabled_slots || [];
}

_tag.load({
  name: "freestar",
  src: "https://a.pub.network/almanac-com/pubfig.min.js",
  async: true,
  dataset: {
    cfasync: "false"
  }
}, function(tag) {
  initFreestart()

  // freestar-exclude
  if (tag.includes(["/retail", "/mediakit", "/club", "/spring-weather-forecast", "/planting-map"])) {
    window.freestar.config.disabledProducts = {
      stickyFooter: true,
      dynamicAds: true,
      pushdown: true,
    };
  }

  window.freestar.queue.push(function() {
    googletag.pubads().setTargeting('pageId', '252787');
    googletag.pubads().setTargeting('ofa_topics', '');
    googletag.pubads().setTargeting('ofa_pagesponsor', '');
    googletag.pubads().setTargeting('ofa_pagetype', 'basic_page');
    console.log('--pushed');
  });
  window.freestar.initCallback = function () { (freestar.config.enabled_slots.length === 0) ? freestar.initCallbackCalled = false : freestar.newAdSlots(freestar.config.enabled_slots) }
})
